{
  "$schema": "http://json-schema.org/draft-07/schema#",
  "additionalProperties": false,
  "properties": {
    "global": {
      "description": "Global values are values that can be accessed from any chart or subchart by exactly the same name.",
      "required": [],
      "title": "global",
      "type": "object"
    },
    "listeners": {
      "additionalProperties": false,
      "properties": {
        "tcp": {
          "items":
            {
              "additionalProperties": false,
              "properties": {
                "name": {
                  "default": "some",
                  "required": [],
                  "title": "name",
                  "type": "string",
                  "pattern": "^[a-z]+[a-z0-9-]+[a-z0-9]$"
                },
                "port": {
                  "default": 10000,
                  "required": [],
                  "title": "port",
                  "type": "integer",
                  "minimum": 10000,
                  "maximum": 12000
                }
              },
              "required": [
                "name",
                "port"
              ],
              "type": "object"
            },
          "required": [],
          "title": "tcp",
          "type": "array"
        }
      },
      "required": [
        "tcp"
      ],
      "title": "listeners",
      "type": "object"
    }
  },
  "required": [
    "listeners"
  ],
  "type": "object"
}